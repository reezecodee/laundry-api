{
  "name": "laundry-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prod": "pm2-runtime start dist/index.js -i max",
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d src/config/data-source.ts",
    "migration:gen": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:refresh": "npm run typeorm -- schema:drop && npm run typeorm -- migration:run",
    "seed": "ts-node src/seeds/main.seeder.ts"
  },
  "keywords": [],
  "author": "Muhammad Azmi Avicenna",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pm2": "^6.0.13",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0"
  }
}
